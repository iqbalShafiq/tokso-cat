<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToksoCat" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/light_primary</item>
        <item name="colorPrimaryVariant">@color/light_primary</item>
        <item name="colorOnPrimary">@color/light_primary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/red_primary</item>
        <item name="colorSecondaryVariant">@color/green_primary</item>
        <item name="colorOnSecondary">@color/red_primary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>
</resources>